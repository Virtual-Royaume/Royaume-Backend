{
  "version": "3.2.0",
  "main": "./build/src/Index.js",
  "scripts": {
    "start": "node .",
    "dev": "npx nodemon -L --exec npx ts-node ./src/Index.ts",
    "build": "npx tsc && npx copyfiles ./resources/**/**/*.* ./build/",
    "client": "npx ts-node ./src/others/ClientTester.ts",
    "eslint": "npx eslint src/**/*.ts --fix",
    "gen-gql": "npx graphql-codegen --config codegen.yml",
    "gen-token": "npx ts-node ./src/others/TokenGenerator.ts"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.11.3",
    "@graphql-codegen/typescript-operations": "^2.5.2",
    "@graphql-codegen/typescript-resolvers": "^2.7.2",
    "@swc/core": "^1.2.223",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.21.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^2.9.1",
    "@graphql-tools/graphql-file-loader": "^7.5.0",
    "@graphql-tools/load": "^7.7.1",
    "apollo-server": "^3.10.0",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "mongodb": "^4.8.1"
  }
}
